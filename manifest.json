{
  "manifest_version": 3,
  "name": "Petpets",
  "description": "Petpets - A web extension for virtual petpet adoption",
  "version": "1.0.1",
  "action":{
        "default_popup": "index.html",
        "default_title": "Latest Covid Report"
  },
  "content_scripts": [
    {
      "matches": ["https://www.jahneer.me/*"],
      "js": ["script.js"]
    }
  ],
  "permissions": [
    "*://*/*",
    "<all_urls>"
  ]
}